%ul
  - YAML.load(version.object_changes).each do |attribute, change|
    %li
      %em= attribute
      - if change.is_a? Array
        changed from
        %em
          "#{change[0]}"
        to
        = succeed "." do
          %em
            "#{change[1]}"
      - else
        set to
        %em
          "#{change}"
  - end if version.object_changes
